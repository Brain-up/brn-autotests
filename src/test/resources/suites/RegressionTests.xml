<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="RegressionTests" parallel="methods" thread-count="4" verbose="1">
    <test name = "ContractTests">
        <packages>
            <package name="contractTests.*"/>
        </packages>
    </test>
    <test name = "DataTests" parallel="none">
        <packages>
            <package name="dataTests.*"/>
        </packages>
    </test>
    <test name = "UiTests" >
        <packages>
            <package name="ui.*"/>
        </packages>
    </test>
</suite>
