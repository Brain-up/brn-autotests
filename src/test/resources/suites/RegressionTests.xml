<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="RegressionTests" >
    <test name = "ContractTests"  parallel="methods">
        <packages>
            <package name="contractTests.*"/>
        </packages>
    </test>
    <test name = "DataTests" parallel="false">
        <packages>
            <package name="dataTests.*"/>
        </packages>
    </test>
    <test name = "UiTests" parallel="false" >
        <packages>
            <package name="ui.*"/>
        </packages>
    </test>
</suite>
